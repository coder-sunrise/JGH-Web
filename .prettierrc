{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 80,
  "semi": false,
  "tabWidth": 2,
  "spaceBeforeFunctionParen": true,
  "bracketSpacing": true,
  "jsxSingleQuote": true,
  "arrayExpand": true,
  "useTabs": false,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    }
  ]
}
